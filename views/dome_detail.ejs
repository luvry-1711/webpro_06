<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title><%= data.name %></title>
</head>
<body>
  <h2><%= data.name %>の詳細</h2>
  <table>
    <tr><th>項目</th><th>データ</th></tr>
    <tr><td>場所</td><td><%= data.place %></td></tr>
    <tr><td>ドーム</td><td><%= data.name %></td></tr>
    <tr><td>最寄り駅</td><td><%= data.station %></td></tr>
    <tr><td>キャパ</td><td><%= data.capa %></td></tr>
    <tr><td>ゲート</td><td><a href="/dome_detail/<%= id %>">ゲート一覧</a></td></tr>
  </table>
  <a href="/dome/edit/<%= id %>">編集</a>
  <a href="/dome">ドーム一覧に戻る</a>
</body>
</html>