<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>ハイキュー</title>
</head>
<body>
  <h2>ハイキュー高校一覧</h2>
  <table>
    <tr><th>ID</th><th>都道府県</th><th>高校</th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr>
          <td><%= row.id %></td>
          <td><%= row.place %></td>
          <td><a href="/volleyball/<%= number %>"><%= row.school %></a></td>
        </tr>
        <% number += 1 %>
      <% } %>
  </table>
</body>
</html>